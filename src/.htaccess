RewriteEngine On
RewriteBase /
RewriteRule ^([^/]*\.css)$ Styles/$1 [END]
RewriteRule ^([^/]*\.js)$ Scripts/$1 [END]
RewriteRule ^([^/]*\.(jpg|png))$ Images/$1 [END]

#RewriteRule ^/$ index.php
RewriteRule ^([a-zA-Z]+)/ index.php?action=$1 [QSA]
#RewriteRule ^([a-zA-Z]+)/(.+)/(.+)/$ &$2=$3 
